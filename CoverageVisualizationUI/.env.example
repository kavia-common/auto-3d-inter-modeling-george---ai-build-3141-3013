# Either set REACT_APP_API_BASE or REACT_APP_BACKEND_URL (no trailing slash)
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=

# Optional: supply API key to include X-API-Key header
REACT_APP_API_KEY=

# Optional: others referenced in container_env
REACT_APP_FRONTEND_URL=
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=1
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=0
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/api/v1/healthz
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=0
